function right(e) {
if (navigator.appName == 'Netscape' && 
(e.which == 3 || e.which == 2))
return false;
else if (navigator.appName == 'Microsoft Internet Explorer' && 
(event.button == 2 || event.button == 3)) {
alert("Calculs Online");
return false;
}
return true;
}
document.onmousedown=right;
if (document.layers) window.captureEvents(Event.MOUSEDOWN);
window.onmousedown=right;
// End -->

function getmin(time) {
if (time/60<1) mins = 0;
 else {
 mins = parseInt(time/60)};   
 return mins; 
}

function getsec(time) {
if (time/60<1) mins = 0;
 else {
 mins = parseInt(time/60)};   
  secs = parseInt(time-60*mins);
  return secs;    
}

function computeForm(form){ 

var vma = form.vitma.value*1000;
var pourcent =form.pcent.value/100;
var dist = form.distance.value;
var vit = vma*pourcent;
var tmps = dist/vit*3600
var tmpskm = 3600/(vit/1000);

tm = getmin(tmps);
ts =getsec(tmps);

tm1 = getmin(tmpskm);
ts1 =getsec(tmpskm);


form.minutes.value = tm;
form.secondes.value = ts;

form.minute0.value = tm1;
form.seconde0.value = ts1;


 } 

function clearForm(form) { 
}
