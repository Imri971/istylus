<script Language="VBScript"><!--
function FrontPage_Form1_onsubmit()
  Set theForm = document.FrontPage_Form1

  If (Len(theForm.weight.value) > 3) Then
    MsgBox "Tapez au plus 3 caractères dans le champ ""weight"".", 0, "Erreur de validation"
    theForm.weight.focus()
    FrontPage_Form1_onsubmit = False
    Exit Function
  End If

  checkOK = "0123456789-" 
  checkStr = theForm.weight.value
  allValid = True
  decPoints = 0
  allNum = ""
  For i = 1 to len(checkStr)
    ch = Mid(checkStr, i, 1)
    If (InStr(checkOK, ch) = 0) Then
        allValid = False
        Exit For
    End If
    allNum = allNum & ch
  Next
  If (Not allValid) Then
    MsgBox "Ne tapez que  chiffre caractères dans le champ ""weight"".", 0, "Erreur de validation"
    theForm.weight.focus()
    FrontPage_Form1_onsubmit = False
    Exit Function
  End If

  If ((checkstr <> "" and Not IsNumeric(allNum)) Or (decPoints > 1)) Then
    MsgBox "Tapez un nombre valide dans le champ ""weight"".", 0, "Erreur de validation"
    theForm.weight.focus()
    FrontPage_Form1_onsubmit = False
    Exit Function
  End If

  prsVal = allNum
  If ((prsVal <> "") And (Not (prsVal > 1))) Then
    MsgBox "Tapez une valeur supérieure à ""1"" dans le champ ""weight"".", 0, "Erreur de validation"
    theForm.weight.focus()
    FrontPage_Form1_onsubmit = False
    Exit Function
  End If

  If (theForm.minutes.value = "") Then
    MsgBox "Tapez une valeur pour le champ ""minutes"".", 0, "Erreur de validation"
    theForm.minutes.focus()
    FrontPage_Form1_onsubmit = False
    Exit Function
  End If

  If (Len(theForm.minutes.value) > 4) Then
    MsgBox "Tapez au plus 4 caractères dans le champ ""minutes"".", 0, "Erreur de validation"
    theForm.minutes.focus()
    FrontPage_Form1_onsubmit = False
    Exit Function
  End If

  checkOK = "0123456789-" 
  checkStr = theForm.minutes.value
  allValid = True
  decPoints = 0
  allNum = ""
  For i = 1 to len(checkStr)
    ch = Mid(checkStr, i, 1)
    If (InStr(checkOK, ch) = 0) Then
        allValid = False
        Exit For
    End If
    allNum = allNum & ch
  Next
  If (Not allValid) Then
    MsgBox "Ne tapez que  chiffre caractères dans le champ ""minutes"".", 0, "Erreur de validation"
    theForm.minutes.focus()
    FrontPage_Form1_onsubmit = False
    Exit Function
  End If

  If ((checkstr <> "" and Not IsNumeric(allNum)) Or (decPoints > 1)) Then
    MsgBox "Tapez un nombre valide dans le champ ""minutes"".", 0, "Erreur de validation"
    theForm.minutes.focus()
    FrontPage_Form1_onsubmit = False
    Exit Function
  End If

  prsVal = allNum
  If ((prsVal <> "") And (Not (prsVal > 0))) Then
    MsgBox "Tapez une valeur supérieure à ""0"" dans le champ ""minutes"".", 0, "Erreur de validation"
    theForm.minutes.focus()
    FrontPage_Form1_onsubmit = False
    Exit Function
  End If
  FrontPage_Form1_onsubmit = True 
End Function
--></script>

