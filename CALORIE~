<script language="JavaScript">

function calcul(weight,minutes,scalefactor)

{

calories=(Math.round(weight*minutes*scalefactor));
return calories;

}

function compute(form)

{

var w=form.weight.value*2.204
var m=form.minutes.value*1


form.marche.value=calcul(w,m,0.0188)
form.marcheF.value=calcul(w,m,0.0178)
form.marche1.value=calcul(w,m,0.0292)
form.marche1F.value=calcul(w,m,0.0277)
form.jogging.value=calcul(w,m,0.0786)
form.joggingF.value=calcul(w,m,0.075)
form.jogging1.value=calcul(w,m,0.0968)
form.jogging1F.value=calcul(w,m,0.0924)
form.jogging2.value=calcul(w,m,0.11495)
form.jogging2F.value=calcul(w,m,0.1097)
form.jogging3.value=calcul(w,m,0.2238)
form.jogging3F.value=calcul(w,m,0.2136)
form.stepper.value=calcul(w,m,0.0236)
form.stepperF.value=calcul(w,m,0.0165)
form.stepper1.value=calcul(w,m,0.0605)
form.stepper1F.value=calcul(w,m,0.0577)
form.velo.value=calcul(w,m,0.0399)
form.veloF.value=calcul(w,m,0.038)
form.velo1.value=calcul(w,m,0.04175)
form.velo1F.value=calcul(w,m,0.0398)
form.aerobic.value=calcul(w,m,0.088)
form.aerobicF.value=calcul(w,m,0.0867)
form.natation.value=calcul(w,m,0.04085)
form.natationF.value=calcul(w,m,0.0388)
form.natation1.value=calcul(w,m,0.121)
form.natation1F.value=calcul(w,m,0.1154)
form.ski.value=calcul(w,m,0.0544)
form.skiF.value=calcul(w,m,0.0519)
form.tennis.value=calcul(w,m,0.050)
form.tennisF.value=calcul(w,m,0.0476)
form.alpinisme.value=calcul(w,m,0.0605)
form.alpinismeF.value=calcul(w,m,0.0577)
form.basketball.value=calcul(w,m,0.063)
form.basketballF.value=calcul(w,m,0.0603)
form.danse.value=calcul(w,m,0.0343)
form.danseF.value=calcul(w,m,0.0327)
form.lutte.value=calcul(w,m,0.0786)
form.lutteF.value=calcul(w,m,0.075)
form.couper.value=calcul(w,m,0.0484)
form.couperF.value=calcul(w,m,0.0462)
form.bowling.value=calcul(w,m,0.021)
form.bowlingF.value=calcul(w,m,0.0178)
form.musculation.value=calcul(w,m,0.041)
form.musculationF.value=calcul(w,m,0.030)
form.pingpong.value=calcul(w,m,0.0348)
form.pingpongF.value=calcul(w,m,0.0331)
form.aviron.value=calcul(w,m,0.0968)
form.avironF.value=calcul(w,m,0.0924)
form.football.value=calcul(w,m,0.0968)
form.footballF.value=calcul(w,m,0.0924)
form.course.value=calcul(w,m,0.028)
form.courseF.value=calcul(w,m,0.0251)
form.assis.value=calcul(w,m,0.009)
form.assisF.value=calcul(w,m,0.0084)
form.sommeil.value=calcul(w,m,0.00705)
form.sommeilF.value=calcul(w,m,0.006702)   
form.bureau.value=calcul(w,m,0.0106)
form.bureauF.value=calcul(w,m,0.01)
form.conduire.value=calcul(w,m,0.0141)
form.conduireF.value=calcul(w,m,0.0134)
form.bricolage.value=calcul(w,m,0.0218)
form.bricolageF.value=calcul(w,m,0.0207)

}

function clearvalues(form)

{

form.weight.value= 0
form.minutes.value=60

form.marche.value=""
form.marcheF.value=""
form.marche1.value=""
form.marche1F.value=""
form.jogging.value=""
form.joggingF.value=""   
form.jogging1.value=""
form.jogging1F.value=""
